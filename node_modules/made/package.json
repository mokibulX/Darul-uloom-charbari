{
  "name": "made",
  "preferGlobal": true,
  "version": "0.0.1",
  "author": "Ron Burch <burchronald@gmail.com>",
  "description": "A simple reactive build tool",
  "contributors": [
    {
      "name": "Ron Burch",
      "email": "burchronald@gmail.com"
    }
  ],
  "bin": {
    "made": "./main.js"
  },
  "scripts": {
    "test": "vows --spec --isolate",
    "pretest": "coffee --compile .; echo '#!/usr/bin/env node'|cat - main.js > tmp.js && mv tmp.js main.js",
    "prepublish": "coffee --compile .; echo '#!/usr/bin/env node'|cat - main.js > tmp.js && mv tmp.js main.js"
  },
  "main": "./main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronbu/made.git"
  },
  "keywords": [
    "cli",
    "buildsystem",
    "automation",
    "reactive"
  ],
  "dependencies": {
    "minimatch": "*",
    "chokidar": "*",
    "coffee-script": "*"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "license": "BSD"
}